    "@nestjs/testing": "^9.0.8",
    "@types/jest": "^28.1.6",
    "@types/supertest": "^2.0.11",
    "jest": "^28.1.3",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.6",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.2.1",
    "typescript": "^4.5.2"
