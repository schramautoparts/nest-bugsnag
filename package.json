{
  "name": "@schramautoparts/nest-bugsnag",
  "version": "9.0.1",
  "description": "A Nest module wrapper for bugsnag",
  "keywords": [
    "nestjs",
    "bugsnag",
    "logger"
  ],
  "engines": {
    "node": ">=12.x"
  },
  "type": "git",
  "url": "https://github.com/schramautoparts/nest-bugsnag",
